#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x569bbd724960 .scope module, "pc_reg_tb" "pc_reg_tb" 2 6;
 .timescale -9 -9;
v0x569bbd73d7c0_0 .var "clk", 0 0;
v0x569bbd73d880_0 .var "pc_next_i", 31 0;
v0x569bbd73d950_0 .net "pc_o", 31 0, v0x569bbd73d460_0;  1 drivers
v0x569bbd73da50_0 .var "pc_write_en_i", 0 0;
v0x569bbd73db20_0 .var "rst_n", 0 0;
S_0x569bbd724af0 .scope module, "DUT" "pc_reg" 2 16, 3 4 0, S_0x569bbd724960;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 32 "pc_next_i";
    .port_info 3 /INPUT 1 "pc_write_en_i";
    .port_info 4 /OUTPUT 32 "pc_o";
v0x569bbd724cf0_0 .net "clk", 0 0, v0x569bbd73d7c0_0;  1 drivers
v0x569bbd73d380_0 .net "pc_next_i", 31 0, v0x569bbd73d880_0;  1 drivers
v0x569bbd73d460_0 .var "pc_o", 31 0;
v0x569bbd73d550_0 .net "pc_write_en_i", 0 0, v0x569bbd73da50_0;  1 drivers
v0x569bbd73d610_0 .net "rst_n", 0 0, v0x569bbd73db20_0;  1 drivers
E_0x569bbd723750/0 .event negedge, v0x569bbd73d610_0;
E_0x569bbd723750/1 .event posedge, v0x569bbd724cf0_0;
E_0x569bbd723750 .event/or E_0x569bbd723750/0, E_0x569bbd723750/1;
    .scope S_0x569bbd724af0;
T_0 ;
    %wait E_0x569bbd723750;
    %load/vec4 v0x569bbd73d610_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x569bbd73d460_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x569bbd73d550_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x569bbd73d380_0;
    %assign/vec4 v0x569bbd73d460_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x569bbd724960;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x569bbd73d7c0_0;
    %inv;
    %store/vec4 v0x569bbd73d7c0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x569bbd724960;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x569bbd73d7c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x569bbd73db20_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x569bbd73d880_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x569bbd73da50_0, 0, 1;
    %vpi_call 2 37 "$dumpfile", "sim/pc_reg_tb.vcd" {0 0 0};
    %vpi_call 2 38 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x569bbd724960 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x569bbd73db20_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 47 "$display", "Time %0t: PC after reset = %h", $time, v0x569bbd73d950_0 {0 0 0};
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x569bbd73d880_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x569bbd73da50_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 55 "$display", "Time %0t: PC after increment 1 = %h", $time, v0x569bbd73d950_0 {0 0 0};
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x569bbd73d880_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 61 "$display", "Time %0t: PC after increment 2 = %h", $time, v0x569bbd73d950_0 {0 0 0};
    %pushi/vec4 256, 0, 32;
    %store/vec4 v0x569bbd73d880_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 67 "$display", "Time %0t: PC after jump = %h", $time, v0x569bbd73d950_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x569bbd73da50_0, 0, 1;
    %pushi/vec4 260, 0, 32;
    %store/vec4 v0x569bbd73d880_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 74 "$display", "Time %0t: PC after attempted write (disabled) = %h", $time, v0x569bbd73d950_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x569bbd73da50_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 80 "$display", "Time %0t: PC after re-enabled write = %h", $time, v0x569bbd73d950_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 84 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
